{
	"name": "Dark+++",
	"colors": {
		"editor.background": "#252525",
		"editor.foreground": "#cccccca5",
		"editor.selectionBackground": "#1443a7a7",
		"editor.selectionForeground": "#ffffff",
		"editor.lineHighlightBackground": "#577e592b",

		"editorGutter.background": "#ffffff02",

		"titleBar.activeBackground": "#ffffff00",
		"titleBar.inactiveBackground": "#ffffff00",
		"titleBar.activeForeground": "#ccccccbe",

		"sideBar.border": "#ffffff23",
		"sideBar.foreground": "#cccccce8",
		"sideBar.background": "#252525",

		"statusBar.foreground": "#ffffff90",
		"statusBar.background": "#ffffff1a",
		"statusBar.noFolderForeground": "#6b6b6b",
		"statusBar.noFolderBackground": "#3e3e3e",

		"tab.activeForeground": "#ccccccd5",
		"tab.activeBackground": "#ffffff15",
		"tab.inactiveBackground": "#ffffff06",
		"tab.activeBorder": "#60a679",

		"editorSuggestWidget.background": "#303030",
		"editorSuggestWidget.highlightForeground": "#c48b29",
		"editorSuggestWidget.selectedBackground": "#208272cc",

		"badge.background": "#ffffff29",

		"activityBar.background": "#ffffff0f",
		"activityBar.foreground": "#cccccc",
		"activityBarBadge.background": "#009835",

		"menu.background": "#363636",
		"menu.foreground": "#c2c2c2",

		"menu.selectionBackground": "#4c5c4f",
		"menu.selectionForeground": "#c3d7c3",

		"focusBorder": "#00ff597a",

		"button.background": "#009835ca",

		"list.activeSelectionBackground": "#00ff591d",
		"list.focusOutline": "#00fb585e",

		"tab.hoverBackground": "#36363694",
		"tab.border": "#154f263f",

		"editor.wordHighlightBackground": "#ffffff14",

		"textLink.foreground": "#00ff8889",
		"textLink.activeForeground": "#00ff88bb",

		"progressBar.background": "#00ff8889",

		"editorBracketHighlight.foreground1": "#cf9b00",
		"editorBracketHighlight.foreground2": "#63c14c",
		"editorBracketHighlight.foreground3": "#4ea6f3",
		"editorBracketHighlight.foreground4": "#19c9b1",
		"editorBracketHighlight.foreground5": "#8b9dff",
		"editorBracketHighlight.foreground6": "#e15454"
	},

	"tokenColors": [
		// Overall highlight support
		{
			"name": "Overall Support",
			"scope": "support",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"name": "Overall Name",
			"scope": [
				"entity.name - markup.heading",
				"meta.export.default",
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#d3c493",
				"fontStyle": ""
			}
		},

		{
			"name": "Overall Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#707070"
			}
		},
		{
			"name": "Overall Type",
			"scope": ["entity.name.type"],
			"settings": { "foreground": "#3dbda3" }
		},
		{
			"name": "Overall Placeholder",
			"scope": ["constant.other.placeholder", "constant.character"],
			"settings": {
				"foreground": "#cf8e48"
			}
		},

		{
			"name": "Overall Enum Type Name",
			"scope": ["entity.name.type.enum"],
			"settings": {
				"foreground": "#4EC9B0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Overall Enums Member Name",
			"scope": ["enum", "variable.other.enummember"],
			"settings": {
				"foreground": "#acccae"
			}
		},
		{
			"name": "Overall Function Declaration",
			"scope": ["entity.name.function", "meta.function - meta.block"],
			"settings": {
				"foreground": "#a9a07d",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Overall Function Parameter",
			"scope": ["variable.parameter", "variable.parameter.function"],
			"settings": {
				"foreground": "#cccccc",
				"fontStyle": ""
			}
		},
		{
			"name": "Overall Function Call",
			"scope": ["support.function"],
			"settings": {
				"foreground": "#aea689",
				"fontStyle": "underline"
			}
		},

		{
			"name": "Overall Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#9eca51"
			}
		},
		{
			"name": "Overall Storage type",
			"scope": ["storage", "storage.type"],
			"settings": {
				"foreground": "#e5a765",
				"fontStyle": ""
			}
		},
		{
			"name": "Overall Storage modifier",
			"scope": ["storage.modifier.package", "storage.modifier.import"],
			"settings": {
				"foreground": "#e6edf3"
			}
		},

		// Variables
		{
			"name": "Overall Variables",
			"scope": ["variable", "variable.other"],
			"settings": {
				"foreground": "#acacac",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"name": "Constants Variables",
			"scope": ["variable.other.constant"],
			"settings": {
				"foreground": "#aca7db",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#79c0ff"
			}
		},

		{
			"name": "Overall Numeric",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#7cc6eb",
				"fontStyle": ""
			}
		},
		{
			"name": "Overall Strings",
			"scope": ["string", "string punctuation.section.embedded source"],
			"settings": {
				"foreground": "#72995b",
				"fontStyle": ""
			}
		},
		// Invalid
		{
			"scope": "invalid.broken",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffa198"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffa198"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffa198"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffa198"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"fontStyle": "italic underline",
				"background": "#ff7b72",
				"foreground": "#f0f6fc",
				"content": "^M"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#ffa198"
			}
		},
		//Regular Expression
		{
			"scope": ["source.regexp", "string.regexp"],
			"settings": {
				"foreground": "#a5d6ff"
			}
		},
		{
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#a5d6ff"
			}
		},
		{
			"scope": "string.regexp constant.character.escape",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7ee787"
			}
		},

		{
			"scope": "meta.module-reference",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#ffa657"
			}
		},
		// Markup Language
		{
			"name": "Overall Tag",
			"scope": ["meta.tag"],
			"settings": {
				"foreground": "#d5cc93"
			}
		},
		{
			"name": "Overall Tag Name",
			"scope": ["entity.name.tag", "support.class.component"],
			"settings": {
				"foreground": "#9ECA51"
			}
		},
		{
			"name": "Overall Property Name",
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"name": "Markup Heading",
			"scope": ["markup.heading"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6fbfd3"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7ee787"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e6edf3"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e6edf3"
			}
		},
		{
			"scope": ["markup.underline"],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": ["markup.strikethrough"],
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"background": "#490202",
				"foreground": "#ffa198"
			}
		},
		{
			"scope": ["punctuation.section.embedded"],
			"settings": {
				"foreground": "#ff7b72"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"background": "#04260f",
				"foreground": "#7ee787"
			}
		},
		{
			"scope": ["markup.changed", "punctuation.definition.changed"],
			"settings": {
				"background": "#5a1e02",
				"foreground": "#ffa657"
			}
		},
		{
			"scope": ["markup.ignored", "markup.untracked"],
			"settings": {
				"foreground": "#161b22",
				"background": "#79c0ff"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#d2a8ff",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#79c0ff"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#79c0ff"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#79c0ff"
			}
		},

		{
			"scope": ["constant.other.reference.link", "string.other.link"],
			"settings": {
				"foreground": "#a5d6ff"
			}
		},

		// Bracket
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#8b949e"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8f0c00"
			}
		},
		// Language specific highlight support
		{
			"name": "JSON property names",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#8cc5e4"
			}
		},
		{
			"name": "C/CPP modifier",
			"scope": ["storage.modifier.c", "storage.modifier.cpp"],
			"settings": {
				"foreground": "#aca7db",
				"fontStyle": "bold"
			}
		}
	]
}
